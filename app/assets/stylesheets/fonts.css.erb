@font-face {
  font-family:'Casper';
  src: url('/fonts/Casper.eot');
  src: url('/fonts/Casper.eot') format('embedded-opentype'),
       url('/fonts/Casper.woff') format('woff'),
       url('/fonts/Casper.otf') format('opentype'),
       url('/fonts/Casper.svg') format('svg');
  font-weight: normal;
  font-style: normal;
}

@font-face {
  font-family: 'Raleway';
  src: url('/fonts/raleway-regular-webfont.eot');
  src: url('/fonts/raleway-regular-webfont.eot?#iefix"') format('embedded-opentype'),
       url('/fonts/raleway-regular-webfont.woff') format('woff'),
       url('/fonts/raleway-regular-webfont.otf') format('opentype'),
       url('/fonts/raleway-regular-webfont.svg#raleway-regular') format('svg');
  font-weight: normal;
  font-style: normal;
}